{
  "rulesDirectory": "node_modules/tslint-eslint-rules/dist/rules",
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "outDir": "../.build/js/",
    "sourceMap": true,
    "target": "es2015",
    "baseUrl": "./src",
    "noImplicitReturns": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src/**/*"
  ]
}

